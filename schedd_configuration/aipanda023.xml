<?xml version="1.0"?>
<submissionhosts>
    <submissionhost hostname="aipanda023.cern.ch" submissionhostenable="True">
                <contacts>
                    <email>forcontact@mail.com</email>
                    <email>forcontact@mail.com</email>
                </contacts>
                <metrics warning_delay="6h" critical_delay="6h">
